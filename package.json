{
  "name": "vue2-tooltip-simple",
  "version": "1.0.4",
  "description": "A simple tooltip to make your life easier!",
  "main": "dist/vue2-tooltip-simple.ssr.js",
  "browser": "dist/vue2-tooltip-simple.esm.js",
  "module": "dist/vue2-tooltip-simple.esm.js",
  "unpkg": "dist/vue2-tooltip-simple.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue",
    "!src/serve-dev.*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JosephWoodside/vue2-tooltip-simple.git"
  },
  "keywords": [
    "vue",
    "vue2",
    "vuejs",
    "front-end",
    "javascript",
    "component",
    "tooltip",
    "simple"
  ],
  "author": "Joseph Woodside <woodside.joseph@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JosephWoodside/vue2-tooltip-simple/issues"
  },
  "homepage": "https://github.com/JosephWoodside/vue2-tooltip-simple#readme",
  "scripts": {
    "serve": "vue-cli-service serve src/serve-dev.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-replace": "^2.2.1",
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "cross-env": "^6.0.3",
    "minimist": "^1.2.0",
    "rollup": "^1.27.13",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-vue": "^5.1.5",
    "sass-loader": "^8.0.2",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "peerDependencies": {
    "vue": "^2.6.10"
  },
  "engines": {
    "node": ">=8"
  }
}
